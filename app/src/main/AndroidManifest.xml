<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.prashant.root.be">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_new"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_new"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".get_blood"
            android:label="@string/details" />
        <activity
            android:name=".blood_banks"
            android:label="@string/blood_banks"
            android:parentActivityName=".get_blood" />
        <activity
            android:name=".DiseaseDetail"
            android:label="@string/disease_detail"
            android:parentActivityName=".FirstAid" />
        <activity
            android:name=".FirstAid"
            android:label="@string/select_problem" />
        <activity
            android:name=".DonateBlood"
            android:label="Guidelines" />
        <activity android:name=".donation_form"></activity>
    </application>

</manifest>